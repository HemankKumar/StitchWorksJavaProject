<AnchorPane prefHeight="660.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.java_project.customer_profile_page.profile_page">
   <children>
      <Label alignment="CENTER" layoutX="62.0" layoutY="123.0" prefHeight="102.0" prefWidth="454.0" text="CUSTOMER ENROLLMENT" underline="true">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <TextField fx:id="name" layoutX="50.0" layoutY="283.0" prefHeight="26.0" prefWidth="186.0" promptText="Name" />
      <TextField fx:id="mobileno" layoutX="280.0" layoutY="283.0" prefHeight="26.0" prefWidth="186.0" promptText="Mobile.No" />
      <TextField fx:id="address" layoutX="50.0" layoutY="415.0" prefHeight="26.0" prefWidth="416.0" promptText="Addess" />
      <ComboBox fx:id="state" editable="true" layoutX="52.0" layoutY="469.0" prefHeight="26.0" prefWidth="210.0" promptText="State" />
      <ComboBox fx:id="city" editable="true" layoutX="52.0" layoutY="523.0" prefHeight="26.0" prefWidth="210.0" promptText="City" />
      <ComboBox fx:id="gender" editable="true" layoutX="50.0" layoutY="339.0" prefHeight="26.0" prefWidth="186.0" promptText="Gender" />
      <DatePicker fx:id="dob" layoutX="280.0" layoutY="339.0" prefHeight="26.0" prefWidth="186.0" promptText="DOB" />
      <Button layoutX="399.0" layoutY="226.0" mnemonicParsing="false" onAction="#dosearch" prefHeight="26.0" prefWidth="70.0" text="Search" />
      <Button layoutX="52.0" layoutY="573.0" mnemonicParsing="false" onAction="#dosave" prefHeight="26.0" prefWidth="76.0" text="Save" />
      <Button layoutX="143.0" layoutY="573.0" mnemonicParsing="false" onAction="#doupdate" prefHeight="26.0" prefWidth="70.0" text="Update" />
      <Button layoutX="229.0" layoutY="573.0" mnemonicParsing="false" onAction="#dodelete" prefHeight="26.0" prefWidth="70.0" text="Delete" />
      <ImageView fx:id="picpath" fitHeight="109.0" fitWidth="144.0" layoutX="354.0" layoutY="428.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="379.0" layoutY="574.0" mnemonicParsing="false" onAction="#douploadprofilepic" text="Upload Profile Pic" />
      <TextField fx:id="email" layoutX="48.0" layoutY="226.0" prefHeight="26.0" prefWidth="330.0" promptText="Email Id" />
      <ImageView fitHeight="129.0" fitWidth="150.0" layoutX="214.0" pickOnBounds="true">
         <image>
            <Image url="@tailor_logo.jpg" />
         </image>
      </ImageView>
   </children>
